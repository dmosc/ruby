{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "google", "prettier"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "plugins": ["react"],
  "rules": {
    "new-cap": 0,
    "require-jsdoc": 0,
    "no-console": ["error", {"allow": ["warn", "error", "info"]}]
  },
  "parser": "babel-eslint"
}
